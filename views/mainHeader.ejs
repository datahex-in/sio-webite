  <script>
    // Get the current URL path
    var path = window.location.pathname;

    // Find the corresponding menu item and add the 'active-menu' class
    $("ul.main-menu li").each(function () {
      var menuItemLink = $(this).find("a").attr("href");
      if (path === menuItemLink) {
        $(this).addClass("active-menu");
      }
    });
  </script>
  <script>
    // Get the current URL path
    var path = window.location.pathname;
    // Find the corresponding bottom bar link and add the 'activeContainer' class
